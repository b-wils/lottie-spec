{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$comment": "List of valid shapes",
    "required": ["ty"],
    "allOf": [
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "el"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/ellipse"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "fl"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/fill"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "gr"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/group"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "sh"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/path"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "sr"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/polystar"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "rc"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/rectangle"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "st"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/stroke"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "tr"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/transform"
            }
        },
        {
            "if": {
                "type": "object",
                "properties": {
                    "ty": {"const": "tm"}
                }
            },
            "then": {
                "$ref": "#/$defs/shapes/trim-path"
            }
        }
    ]
}
